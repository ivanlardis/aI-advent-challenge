<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>

    <!-- Оставляем только предупреждения и ошибки от библиотек -->
    <root level="WARN">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
